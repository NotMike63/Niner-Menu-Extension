{
  "name": "niner-menus",
  "displayName": "Niner Menus",
  "description": "LEAVE BLANK",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:Java"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "niner-menus.downloadJars",
        "title": "Download Jars for 2214"
      },
      {
        "command": "niner-menus.createProject",
        "title": "Create New 2214 Project"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "niner-menus",
          "title": "Niner Menu",
          "icon": "media/Niners.svg"
        }
      ]
    },
    "views": {
      "niner-menus": [
        {
          "id": "niner-file-explorer",
          "name": "Niner File Explorer"
        },
        {
          "id": "menu-title",
          "name": "2214 Commands"
        }
      ]
    }
  },


  
  "scripts": {
    "vscode:prepublish" : "npm run compile",
    "compile": "tsx -p ./",
    "watch": "tsc -watch -[ ./",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
